import math
if __name__ == "__main__":
    t = int(input())
    while t!=0:
        ok =True
        s = input()
        for i in range(len(s)-1):
            if s[i] > s[i+1]:
                ok = False
                break
        if ok == True: print("YES")
        else: print("NO")
        t-=1
